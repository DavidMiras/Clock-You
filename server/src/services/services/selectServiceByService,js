import getPool from "../../db/getPool.js";
import generateErrorUtil from "../../utils/generateErrorUtil.js";


const selectServiceByService = async (service) =>  {
    try {
        const pool = getPool();

        pool.query(
            `
            SELECT id FROM typeOfService WHERE service = ?
            `, [service]
        );

        return service;
    } catch (error) {
        error.message
    }
}

export default selectServiceByService;